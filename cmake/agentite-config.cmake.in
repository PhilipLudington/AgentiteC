@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/agentite-targets.cmake")

check_required_components(agentite)

# Find SDL3 dependency
find_package(PkgConfig QUIET)
if(PkgConfig_FOUND)
    pkg_check_modules(SDL3 IMPORTED_TARGET sdl3)
endif()
if(NOT SDL3_FOUND)
    find_package(SDL3 QUIET)
endif()
